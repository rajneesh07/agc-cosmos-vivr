    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "quotes": "off",
      "semi": "off",
      "no-trailing-spaces": "off",
      "space-before-function-paren": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
